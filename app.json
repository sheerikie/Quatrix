{
  "expo": {
    "name": "Quatrix Logistics App",
    "slug": "Quatrix",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F57C00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quatrix",
      "googleServicesFile": "./config/google-services.json"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Quatrix",
  "name": "Quatrix"
}